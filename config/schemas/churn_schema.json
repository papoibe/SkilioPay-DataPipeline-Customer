{
  "type": "object",
  "properties": {
    "user_id": {
      "type": "string",
      "pattern": "^U[0-9]{5}$",
      "description": "Unique user identifier"
    },
    "age": {
      "type": "integer",
      "minimum": 13,
      "maximum": 100,
      "description": "User age"
    },
    "country": {
      "type": "string",
      "enum": ["Thailand", "Indonesia", "Malaysia", "Philippines", "Vietnam"],
      "description": "User country"
    },
    "city": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "description": "User city"
    },
    "reg_days": {
      "type": "integer",
      "minimum": 0,
      "maximum": 3650,
      "description": "Days since registration"
    },
    "marketing_source": {
      "type": "string",
      "enum": ["organic", "referral", "ads_fb", "ads_ig", "influencer", "other"],
      "description": "Marketing acquisition source"
    },
    "sessions_30d": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of sessions in last 30 days"
    },
    "sessions_90d": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of sessions in last 90 days"
    },
    "avg_session_duration_90d": {
      "type": "number",
      "minimum": 0,
      "description": "Average session duration in seconds"
    },
    "median_pages_viewed_30d": {
      "type": "number",
      "minimum": 0,
      "description": "Median pages viewed per session in 30 days"
    },
    "search_queries_30d": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of search queries in 30 days"
    },
    "device_mix_ratio": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Device mix ratio (mobile vs desktop)"
    },
    "app_version_major": {
      "type": "string",
      "pattern": "^[0-9]+\\.x$",
      "description": "Major app version"
    },
    "orders_30d": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of orders in last 30 days"
    },
    "orders_90d": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of orders in last 90 days"
    },
    "orders_2024": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of orders in 2024"
    },
    "aov_2024": {
      "type": "number",
      "minimum": 0,
      "description": "Average order value in 2024"
    },
    "gmv_2024": {
      "type": "number",
      "minimum": 0,
      "description": "Gross merchandise value in 2024"
    },
    "category_diversity_2024": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of different categories purchased in 2024"
    },
    "days_since_last_order": {
      "type": "integer",
      "minimum": 0,
      "maximum": 365,
      "description": "Days since last order"
    },
    "discount_rate_2024": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Average discount rate in 2024"
    },
    "refunds_count_2024": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of refunds in 2024"
    },
    "refund_rate_2024": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Refund rate in 2024"
    },
    "support_tickets_2024": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of support tickets in 2024"
    },
    "avg_csat_2024": {
      "type": "number",
      "minimum": 1,
      "maximum": 5,
      "description": "Average customer satisfaction score in 2024"
    },
    "emails_open_rate_90d": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Email open rate in last 90 days"
    },
    "emails_click_rate_90d": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Email click rate in last 90 days"
    },
    "review_count_2024": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of reviews written in 2024"
    },
    "avg_review_stars_2024": {
      "type": "number",
      "minimum": 1,
      "maximum": 5,
      "description": "Average review stars in 2024"
    },
    "rfm_recency": {
      "type": "integer",
      "minimum": 0,
      "maximum": 365,
      "description": "RFM recency score"
    },
    "rfm_frequency": {
      "type": "integer",
      "minimum": 0,
      "description": "RFM frequency score"
    },
    "rfm_monetary": {
      "type": "number",
      "minimum": 0,
      "description": "RFM monetary score"
    },
    "churn_label": {
      "type": "integer",
      "enum": [0, 1],
      "description": "Churn label: 0 = not churned, 1 = churned"
    }
  },
  "required": [
    "user_id", "age", "country", "city", "reg_days", "marketing_source",
    "sessions_30d", "sessions_90d", "avg_session_duration_90d",
    "median_pages_viewed_30d", "search_queries_30d", "device_mix_ratio",
    "app_version_major", "orders_30d", "orders_90d", "orders_2024",
    "aov_2024", "gmv_2024", "category_diversity_2024", "days_since_last_order",
    "discount_rate_2024", "refunds_count_2024", "refund_rate_2024",
    "support_tickets_2024", "avg_csat_2024", "emails_open_rate_90d",
    "emails_click_rate_90d", "review_count_2024", "avg_review_stars_2024",
    "rfm_recency", "rfm_frequency", "rfm_monetary", "churn_label"
  ],
  "additionalProperties": false
}
